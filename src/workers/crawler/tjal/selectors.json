{
    "PrimeiroGrau": {
        "classe": {
            "selector": "#classeProcesso",
            "treatment": "text"
        },
        "area": {
            "selector": "#areaProcesso span",
            "treatment": "text"
        },
        "assunto": {
            "selector": "#assuntoProcesso",
            "treatment": "text"
        },
        "dataDistribuicao": {
            "selector": "#dataHoraDistribuicaoProcesso",
            "treatment": "split",
            "splitChar": "-"
        },
        "juiz": {
            "selector": "#juizProcesso",
            "treatment": "text"
        },
        "valorAcao": {
            "selector": "#valorAcaoProcesso",
